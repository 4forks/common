{
  "name": "payconn/common",
  "description": "Payconn payment processing library for PHP",
  "keywords": [
    "payconn",
    "gateway",
    "payment",
    "purchase",
    "3d",
    "turkey"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Murat SAC",
      "email": "muratsac@mail.com"
    }
  ],
  "type": "library",
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Payconn\\Common\\": "src/Common"
    },
    "classmap": ["src/Payconn.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Payconn\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "phpstan/phpstan": "^0.11.2"
  }
}
